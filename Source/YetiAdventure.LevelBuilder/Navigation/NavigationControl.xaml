<UserControl x:Class="YetiAdventure.LevelBuilder.Navigation.NavigationControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:YetiAdventure.LevelBuilder.Navigation"
             mc:Ignorable="d"
             d:DesignHeight="300"
             d:DesignWidth="300">
    <Grid DataContext="{Binding RelativeSource={RelativeSource AncestorType={x:Type local:NavigationControl}}}">
        <Frame x:Name="navFrame"
               NavigationUIVisibility="Hidden" />
    </Grid>
</UserControl>
